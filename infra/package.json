{
  "name": "banca-internet-infra",
  "version": "1.0.0",
  "description": "Infraestructura AWS CDK para Banca por Internet",
  "main": "lib/app.js",
  "scripts": {
    "build": "tsc",
    "build:lambdas": "node build-lambdas.js",
    "build:all": "npm run build && npm run build:lambdas",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "bootstrap": "cdk bootstrap",
    "deploy": "npm run build:all && cdk deploy --all --require-approval never",
    "destroy": "cdk destroy --all --force",
    "seed": "node dist/scripts/seed-data.js"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/jest": "^29.5.5",
    "@types/node": "20.5.0",
    "aws-cdk": "2.100.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "~5.1.3"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.400.0",
    "@aws-sdk/lib-dynamodb": "^3.400.0",
    "@types/uuid": "^9.0.4",
    "aws-cdk-lib": "2.100.0",
    "aws-sdk": "^2.1457.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0"
  }
}
